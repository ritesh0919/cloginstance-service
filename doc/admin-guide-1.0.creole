== $catalina.base/conf/server.xml
<?xml version='1.0' encoding='utf-8'?>

<Server port="12345" shutdown="SHUTDOWN">

  <Listener className="org.apache.catalina.core.JasperListener" />
  <Listener className="org.apache.catalina.core.JreMemoryLeakPreventionListener" />
  <Listener className="org.apache.catalina.mbeans.ServerLifecycleListener" />
  <Listener className="org.apache.catalina.mbeans.GlobalResourcesLifecycleListener" />

  <Service name="Catalina">

    <!-- 
         Controls the max number of requests the server will handle
           maxThreads - max number of concurrent connections 
           maxQueueSize - max number of requests that will be queued if all threads are full
    -->
    <Executor name="tomcatThreadPool" namePrefix="catalina-exec-" 
              maxThreads="${tomcat.maxThreads}" maxQueueSize="${tomcat.maxQueueSize}"
              maxIdleTime="300000" minSpareThreads="4" />


    <Connector address="${tomcat.address}" port="${tomcat.port}" executor="tomcatThreadPool" 
               protocol="org.apache.coyote.http11.Http11Protocol"
               maxHeaderCount="10" maxHttpHeaderSize="8192" maxParameterCount="10" maxPostSize="10485760"               
               connectionTimeout="5000" />


    <Engine name="Catalina" defaultHost="localhost">
      <Host name="localhost"  appBase="${app.home}/webapp"
            unpackWARs="false" autoDeploy="true"/>
    </Engine>

  </Service>

</Server>

