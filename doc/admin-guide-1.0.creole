== cloginstance.init script
#!/bin/sh

OWNER=covapp

APP_NAME=cloginstance
APP_HOME="/a01/apps/${APP_NAME}"
APP_CONF="${APP_HOME}/conf/app.conf"
APP_GC_LOG="${APP_HOME}/logs/tomcat-gc.log"
APP_ADDR=127.0.0.1
APP_PORT=00000
APP_JMX_PORT=00000
APP_MEMORY=128M
APP_REQ_THREADS=128
APP_REQ_QUEUE=256

ECHO="/bin/echo"
PS="/bin/ps"
SUDO="/usr/bin/sudo"

JAVA_HOME="/a01/software/jdk1.7"
TOMCAT_HOME="/a01/software/tomcat"
CATALINA_BASE="${APP_HOME}/catalina"
CATALINA_OPTS="-server -Xmx${APP_MEMORY}"
CATALINA_OPTS="${CATALINA_OPTS} -XX:-DisableExplicitGC -Djava.net.preferIPv4Stack=true"
CATALINA_OPTS="${CATALINA_OPTS} -XX:+UseConcMarkSweepGC -Xloggc:${APP_GC_LOG} -XX:+PrintGCTimeStamps"
CATALINA_OPTS="${CATALINA_OPTS} -Dcom.sun.management.jmxremote -Dcom.sun.management.jmxremote.port=${APP_JMX_PORT} -Dcom.sun.management.jmxremote.authenticate=false -Dcom.sun.management.jmxremote.ssl=false"
CATALINA_OPTS="${CATALINA_OPTS} -Dtomcat.address=${APP_ADDR} -Dtomcat.port=${APP_PORT} -Dtomcat.maxRequestThreads=${APP_REQ_THREADS} -Dtomcat.maxRequestQueue=${APP_REQ_QUEUE}"
CATALINA_OPTS="${CATALINA_OPTS} -Dapp.home=${APP_HOME} -Dapp.conf=${APP_CONF} -Dspring.profiles.active=prod"
CATALINA_TEMP="${CATALINA_BASE}/temp"

umask 022

#----------- The Actual Meat of the Script Grab a Fork ------------
case $1 in

start)
         cd "${APP_HOME}/logs"

         ${SUDO} -u ${OWNER} JAVA_HOME=${JAVA_HOME} CATALINA_OPTS="${CATALINA_OPTS}" CATALINA_BASE=${CATALINA_BASE} CATALINA_TMPDIR=${CATALINA_TEMP} $TOMCAT_HOME/bin/startup.sh $JAVA_ARGS

         cd -
         ;;

stop)
         ${SUDO} -u ${OWNER} JAVA_HOME=${JAVA_HOME} CATALINA_OPTS="${CATALINA_OPTS}" CATALINA_BASE=${CATALINA_BASE} $TOMCAT_HOME/bin/shutdown.sh
         ;;

status)
         ;;

check)
        $PS auxwww | grep ${APP_GC_LOG} | grep -v "sh -c" | grep -v grep
        ;;

*)
        $ECHO "usage: $0 [ start | stop | check ]"
        ;;

esac

exit 0


